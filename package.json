    "release": "turbo run build --filter=./packages/* && changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.27.0",
    "lefthook": "^1.6.0",
    "turbo": "^2.3.0",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.15.2",
  "engines": {
    "node": ">=22"
  }
}
